{
  "kind": "Role",
  "name": "analyst",
  "dropIfExists": true,
  "privileges": [
    {
      "graph": "movies",
      "access": true,
      "rules": [
        {
          "target": "node",
          "labels": "*",
          "action": "match",
          "resource": "all_properties",
          "access": "GRANTED"
        },
        {
          "target": "relationship",
          "labels": "*",
          "action": "read",
          "resource": "*",
          "access": "GRANTED"
        },
        {
          "target": "relationship",
          "labels": "*",
          "action": "traverse",
          "resource": "*",
          "access": "GRANTED"
        },
        {
          "target": "node",
          "labels": "Person",
          "action": "read",
          "resource": "born",
          "access": "DENIED"
        }
      ]
    }
  ]
}
