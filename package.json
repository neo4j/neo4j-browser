{
  "name": "neo4j-browser",
  "version": "1.0.0",
  "description": "Enjoy your data",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dashboard -t 'Neo4j Browser' -- webpack-dev-server --colors --no-info",
    "starts": "webpack-dashboard -t 'Neo4j Browser' -- webpack-dev-server --https --colors --no-info",
    "lint": "standard --fix",
    "test": "npm run lint && jest",
    "dev": "jest --watch",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "jar": "yarn build && mvn package"
  },
  "jest": {
    "testPathIgnorePatterns": [
      ".jsx$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "neo4j": "<rootDir>/__mocks__/neo4j.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^browser-styles(.*)$": "<rootDir>/src/browser/styles$1",
      "^browser-components(.*)$": "<rootDir>/src/browser/components$1"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/src/shared"
    ]
  },
  "standard": {
    "ignore": [
      "node_modules",
      "build",
      "dist",
      "src/browser/external"
    ],
    "globals": [
      "it",
      "describe",
      "test",
      "expect",
      "beforeEach",
      "afterEach",
      "neo",
      "FileReader",
      "Blob"
    ]
  },
  "repository": {},
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "homepage": "https://neo4j.com",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-preact-require": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.0.0",
    "coffee-loader": "^0.7.3",
    "coffee-script": "^1.12.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.0",
    "jest-cli": "^20.0.0",
    "json-loader": "^0.5.4",
    "nock": "^9.0.6",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.6.0",
    "postcss-loader": "^1.3.3",
    "preact-render-to-string": "^3.6.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.2.2",
    "standard": "^8.6.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "ascii-data-table": "^2.1.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.25.2",
    "cypher-codemirror": "1.0.4",
    "dnd-core": "^2.2.4",
    "file-saver": "^1.3.3",
    "firebase": "^3.6.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.0",
    "lodash.debounce": "^4.0.8",
    "neo4j-driver": "^1.2.0",
    "node-noop": "^1.0.0",
    "preact": "^8.1.0",
    "preact-compat": "^3.13.1",
    "preact-redux": "^2.0.0",
    "preact-suber": "^1.0.3",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dropzone": "^3.10.0",
    "react-icons": "^2.2.1",
    "react-swipe": "^5.0.3",
    "react-timeago": "^3.2.0",
    "redux": "^3.5.2",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.0.3",
    "save-as": "^0.1.7",
    "styled-components": "^1.4.4",
    "suber": "^5.0.1",
    "swipe-js-iso": "^2.0.3",
    "uuid": "^3.0.1"
  }
}
