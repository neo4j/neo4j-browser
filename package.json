{
  "name": "neo4j-browser",
  "version": "3.2.1",
  "description": "Enjoy your data",
  "repository": {
    "type": "git",
    "url": "https://github.com/neo4j/neo4j-browser.git"
  },
  "keywords": [],
  "author": "Neo4j Inc.",
  "license": "GPL-3.0",
  "homepage": "https://neo4j.com",
  "main": "dist/index.html",
  "neo4jDesktop": {
    "apiVersion": "^1.2.0"
  },
  "scripts": {
    "start": "webpack-dashboard -t 'Neo4j Browser' -- webpack-dev-server --mode=development --hot --colors --no-info",
    "starts": "webpack-dashboard -t 'Neo4j Browser' -- webpack-dev-server --mode=development --hot --https --colors --no-info",
    "startnodash": "webpack-dev-server --mode=development --hot --colors --no-info",
    "precommit": "lint-staged",
    "format": "prettier-eslint 'src/**/!(*.min).js' 'src/**/*.jsx' --write",
    "lint": "eslint --fix --ext .js --ext .jsx ./",
    "test": "npm run lint && jest",
    "e2e": "cypress run",
    "e2e-local": "CYPRESS_E2E_TEST_ENV=\"local\" cypress run",
    "e2e-local-open": "CYPRESS_E2E_TEST_ENV=\"local\" cypress open",
    "dev": "jest --watch",
    "build": "NODE_ENV=\"production\" webpack --mode production",
    "prepare-jar": "node ./scripts/prepare-mvn-package.js",
    "jar": "yarn build && mvn package",
    "version-pom": "node ./scripts/set-pom-version.js -f ./pom.xml -v $npm_package_version",
    "version": "npm run version-pom && git add ./pom.xml",
    "update-licenses": "sh ./scripts/generate_licenses.sh",
    "now-start": "serve --single ./dist"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier-eslint --write",
        "git add"
      ]
    }
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill.js"
    ],
    "testPathIgnorePatterns": [
      "/e2e_tests/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^neo4j-driver-alias$": "neo4j-driver",
      "^browser-styles(.*)$": "<rootDir>/src/browser/styles$1",
      "^browser-components(.*)$": "<rootDir>/src/browser/components$1",
      "worker-loader": "<rootDir>/__mocks__/workerLoaderMock.js"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/src/shared"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cypress": "^3.0.1",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.4",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest-cli": "^23.1.0",
    "js2xmlparser": "^3.0.0",
    "lint-staged": "^4.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "ncp": "^2.0.0",
    "nock": "^9.0.14",
    "postcss": "^6.0.23",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "precss": "^2.0.0",
    "prettier-eslint": "^7.1.0",
    "prettier-eslint-cli": "^4.3.0",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.3",
    "react-testing-library": "^4.0.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.2.3",
    "serve": "6.5.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4",
    "worker-loader": "^2.0.0",
    "xml2js": "^0.4.19"
  },
  "dependencies": {
    "ascii-data-table": "^2.1.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.29.0",
    "cypher-codemirror": "^1.0.5",
    "dnd-core": "^2.5.1",
    "file-saver": "^1.3.8",
    "firebase": "^4.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonic": "^0.3.0",
    "lodash.debounce": "^4.0.8",
    "neo4j-driver": "^1.6.1",
    "react": "^16.4.0",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-dnd": "^2.5.1",
    "react-dnd-html5-backend": "^2.5.1",
    "react-dom": "^16.4.0",
    "react-dropzone": "^4.1.2",
    "react-icons": "^2.2.1",
    "react-redux": "^5.0.7",
    "react-suber": "1.0.4",
    "react-timeago": "^3.4.3",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.4.2",
    "save-as": "^0.1.7",
    "semver": "^5.5.0",
    "styled-components": "^3.2.0",
    "stylis": "^3.4.10",
    "suber": "^5.0.1",
    "swipe-js-iso": "^2.0.4",
    "url-parse": "^1.1.9",
    "uuid": "^3.0.1"
  }
}
