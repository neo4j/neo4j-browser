    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-suber": "1.0.4",
    "react-svg-inline": "^2.1.1",
    "react-table": "7.8.0",
    "react-timeago": "^7.2.0",
    "redux": "^5.0.1",
    "regenerator-runtime": "^0.14.1",
    "remark-gfm": "^4.0.0",
    "rxjs": "^7.8.1",
    "semantic-ui-react": "^2.1.5",
    "semver": "^7.6.3",
    "styled-components": "^6.1.13",
