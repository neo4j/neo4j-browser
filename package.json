    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "deepmerge": "^4.3.1",
    "file-saver": "^2.0.5",
    "firebase": "^11.0.2",
