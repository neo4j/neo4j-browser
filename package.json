    "react-svg-inline": "^2.1.1",
    "react-timeago": "^4.1.9",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "regenerator-runtime": "^0.13.2",
    "rxjs": "^5.4.2",
    "semantic-ui-react": "^0.88.0",
    "semver": "^5.5.0",
    "styled-components": "^4.0.0",
    "suber": "^5.0.1",
    "unist-util-visit": "^2.0.2",
    "url-parse": "^1.1.9",
    "uuid": "^3.0.1"
  }
}
